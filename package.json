{
  "name": "babel-plugin-jsx-asset-url-import",
  "type": "module",
  "version": "1.0.0",
  "description": "Transform static url to import in JSX",
  "scripts": {
    "build": "tsc && tsup",
    "lint": "eslint .",
    "test": "jest",
    "prepare": "husky install"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "author": "NoDocCat <nodoccat@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/NoDocCat/babel-plugin-jsx-asset-url-import#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoDocCat/babel-plugin-jsx-asset-url-import.git"
  },
  "bugs": {
    "url": "https://github.com/NoDocCat/babel-plugin-jsx-asset-url-import/issues"
  },
  "keywords": [
    "babel",
    "babel-plugin",
    "jsx"
  ],
  "packageManager": "pnpm@7.27.0",
  "dependencies": {
    "srcset-parse": "^1.1.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.20.12"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/babel__core": "^7.20.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "babel-plugin-tester": "^11.0.4",
    "eslint": "^8.33.0",
    "eslint-config-standard": "^17.0.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.0",
    "ts-jest": "^29.0.5",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  }
}
